{
    "name": "xtwoend/hyperf-tinker",
    "description": "A Powerful REPL for the Hyperf framework.",
    "keywords": ["hyperf", "tinker", "repl", "psysh"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gang Wu",
            "email": "gokure@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2|^8.0",
        "ext-swoole": ">=4.4",
        "hyperf/contract": "^2.0|^3.0",
        "hyperf/di": "^2.0|^3.0",
        "hyperf/command": "^2.0|^3.0",
        "psy/psysh": "^1.0"
    },
    "require-dev": {
        "hyperf/database": "^2.0|^3.0",
        "hyperf/testing": "^2.0|^3.0",
        "mockery/mockery": "^1.4"
    },
    "suggest": {
        "hyperf/database": "The Hypyerf Database package (^2.0||^3.0)."
    },
    "autoload": {
        "psr-4": {
            "Gokure\\HyperfTinker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gokure\\HyperfTinker\\Tests\\": "tests/",
            "App\\": "tests/fixtures/app",
            "One\\Two\\": "tests/fixtures/vendor/one/two"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "hyperf": {
            "config": "Gokure\\HyperfTinker\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
